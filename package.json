{
  "name": "xlsx-to-csv-ts",
  "version": "1.0.2",
  "description": "A simple script to convert .xlsx file to .csv file, with the ability to filter / rename columns",
  "homepage": "https://joesiu.github.io/xlsx-to-csv-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoeSiu/xlsx-to-csv-ts.git"
  },
  "bugs": {
    "url": "https://github.com/JoeSiu/xlsx-to-csv-ts/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "xlsx",
    "csv",
    "typescript"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepare": "husky install",
    "release": "bumpp --commit --tag --push",
    "docs": "typedoc --options typedoc.json"
  },
  "author": {
    "name": "Joe Siu",
    "url": "https://github.com/JoeSiu"
  },
  "licenses": [
    {
      "type": "GNU GPLv3",
      "url": "http://www.gnu.org/licenses/"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "bumpp": "^9.2.1",
    "husky": "^8.0.3",
    "prettier": "3.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.7",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  }
}
